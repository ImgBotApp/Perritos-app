<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="itesm.mx.perritos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pet.PetDetailActivity" />
        <activity android:name=".event.AddEventActivity" />
        <activity android:name=".news.AddNewsActivity" />
        <activity android:name=".event.EventDetailActivity" />
        <activity android:name=".news.NewsDetailActivity" />
        <activity android:name=".product.AddProductActivity" />
        <activity android:name=".product.ProductDetailActivity" />
        <activity android:name=".pet.AddPetActivity"></activity>
    </application>

</manifest>